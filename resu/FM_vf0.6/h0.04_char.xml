<?xml version="1.0" encoding="UTF-8"?>
<Loading_Output>

<!-- OUPUT QUANTITIES -->
<Output>
    <vtk_StressStrain Strain="1" Stress="1"/>
    <vtk_IntVarList numM="1">  </vtk_IntVarList>
    <vtk_IntVarList numM="2">  </vtk_IntVarList>
</Output>


<!--initial values of temperature and external loads-->
<InitLoadExt>
    <T Value="0"/>
    <!--Param Index="1" Value="0.5"/-->
</InitLoadExt>

<!-- SUCCESSIVE LOADINGS AND OUTPUT TIMES -->

<Loading Tag="1">
    <Time_Discretization Discretization="Linear" Nincr="1" Tfinal="10" />
    <Output_vtkList> 1 </Output_vtkList> 
    <!--mechanical load-->
    <xx Driving="Stress" Evolution="Constant"/>
    <yy Driving="Stress" Evolution="Constant"/>
    <zz Driving="Stress" Evolution="Constant"/>
    <xy Driving="Stress" Evolution="Constant"/>
    <xz Driving="Stress" Evolution="Constant"/>
    <yz Driving="Stress" Evolution="Constant"/>
    <!--temperature-->
    <T Evolution="Linear" Value="-190"/>
    <!--other loads-->
    <!--Param Index="1" Evolution="Linear" Value="1"/-->
</Loading>

</Loading_Output>

